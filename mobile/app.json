{
  "expo": {
    "name": "Scrum Board",
    "slug": "scrum-board",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0e1117"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.scrumboard.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0e1117"
      },
      "package": "com.scrumboard.mobile"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/icon.png"
    },
    "scheme": "scrumboard",
    "plugins": [
      "expo-asset"
    ]
  }
}
